{
  "name": "@shesha-io/packages",
  "version": "1.0.0",
  "author": "Shesha",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "start": "SET NODE_ENV=development BABEL_ENV=development styleguidist server --open",
    "styleguide:build": "SET NODE_ENV=production BABEL_ENV=production styleguidist build",
    "build": "run-script-os",
    "build:win32": "rimraf dist && SET NODE_ENV=production BABEL_ENV=production npm run build:log && rollup -c",
    "build:default": "rm -rf dist && NODE_ENV=production BABEL_ENV=production npm run build:log && rollup -c",
    "build:watch": "npm run build:log && rollup -c -w",
    "build:log": "echo \"Building\"",
    "lint": "npm run check-types && npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint . --ext .js,.jsx,.tsx,.ts",
    "eslint:fix": "eslint . --ext .js,.jsx,.tsx,.ts --fix",
    "stylelint": "stylelint \"**/*.{css,scss,sass}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "check-types": "tsc",
    "prepare": "npm run build",
    "prerelease": "npm run lint:fix",
    "release": "npm publish",
    "predeploy": "npm run styleguide:build",
    "deploy": "gh-pages -d styleguide"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=5.5.0",
    "antd": ">=5.22.0",
    "lodash": "^4.17.21",
    "next": ">=15.0.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "axios": "^1.7.8",
    "buffer": "^6.0.3",
    "classnames": "^2.5.1",
    "moment": "^2.30.1",
    "nanoid": "^5.0.8",
    "next": "^15.1.0",
    "qs": "^6.13.0",
    "react-use": "^17.5.1",
    "sortablejs": "^1.15.3",
    "styled-components": "^6.1.14",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@ant-design/nextjs-registry": "^1.0.1",
    "@next/bundle-analyzer": "^15.1.0",
    "@next/eslint-plugin-next": "^15.1.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@shesha-io/reactjs": "0.40.0",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/react": "^16.1.0",
    "@types/lodash": "^4.17.13",
    "@types/react": "^19.0.2",
    "@types/redux-actions": "^2.6.8",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/eslint-plugin-tslint": "^6.21.0",
    "@typescript-eslint/parser": "^8.18.1",
    "antd": "5.22.6",
    "dotenv": "^16.4.7",
    "eslint": "^9.16.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-storybook": "^0.10.2",
    "husky": "^9.1.7",
    "isomorphic-unfetch": "^4.0.2",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nock": "^13.5.6",
    "prettier": "^3.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redux-actions": "^2.6.5",
    "rimraf": "^6.0.1",
    "rollup": "^4.28.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-url": "^3.0.1",
    "run-script-os": "^1.1.6",
    "typescript": "^5.7.2"
  }
}
